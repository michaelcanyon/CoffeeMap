@page
@model CoffeeMapServer.Views.Admin.AddRoasterModel
<section class="content-header">
    <h1>
        Create roaster note
    </h1>
</section>
<!-- Main content -->
<section class="content container-fluid">
    <form method="post">
        <div class="form-group">
            <label for="Name">Name</label>
            <input type="text" class="form-control" asp-for="@Model.roaster.Name">
        </div>
        <button type="submit" class="btn btn-primary">Create</button>
    </form>
</section>



